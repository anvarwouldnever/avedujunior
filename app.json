{
  "expo": {
    "name": "AVEDU Junior",
    "slug": "avedujunior",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/aveduIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/aveduLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.avtech.avedujunior",
      "infoPlist": {
        "CFBundleShortVersionString": "1.0.0",
        "CFBundleVersion": "11",
        "CFBundleName": "AVEDU Junior",
        "CFBundleDisplayName": "AVEDU Junior",
        "ITSAppUsesNonExemptEncryption": false,
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationLandscapeRight",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.avtech.avedujunior"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video",
      "expo-secure-store",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/aveduLogo.png",
          "dark": {
            "image": "./assets/aveduLogo.png",
            "backgroundColor": "#ffffff"
          },
          "imageWidth": 200
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/78991642-a278-4dbf-a0cf-6366710efcb1"
    },
    "owner": "anvarwouldnever3",
    "extra": {
      "eas": {
        "projectId": "78991642-a278-4dbf-a0cf-6366710efcb1"
      }
    }
  }
}
